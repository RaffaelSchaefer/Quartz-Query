{
  "name": "quartz_query",
  "version": "0.0.1",
  "description": "A mineral based search engine for my dad",
  "main": "index.js",
  "scripts": {
    "sass_compile": "npx --no-install sass --no-source-map ./sass/:./css/",
    "typescript_compile": "tsc --build tsconfig.json",
    "minify": "npx --no-install uglifyjs-folder --log-level warning -x .js ./scripts/ -eo ./scripts/",
    "compile": "npm run sass_compile && npm run typescript_compile && npm run minify",
    "test": "npx jest"
  },
  "keywords": [
    "mineral"
  ],
  "author": "Raffael Elias Sch√§fer",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.2",
    "babel-jest": "^27.3.1",
    "jest": "^27.3.1",
    "sass": "^1.43.4",
    "terser": "^5.7.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "uglifyjs-folder": "^2.0.0",
    "electron": "^13.0.1",
    "electron-packager": "^15.2.0"
  }
}
